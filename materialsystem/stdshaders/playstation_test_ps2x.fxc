#include "common_ps_fxc.h"

sampler g_tTestTexture : register( s0 );

struct PS_INPUT
{
	float2 vUv0 : TEXCOORD0;
};

float4 main( PS_INPUT i ) : COLOR
{
	return float4( 0.05, 0.05, 0.05, 1 ) + tex2D( g_tTestTexture, i.vUv0 );
}
